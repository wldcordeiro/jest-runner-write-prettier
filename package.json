{
  "name": "jest-runner-write-prettier",
  "version": "0.0.0-development",
  "description": "A Jest Runner for Prettier that writes the changes.",
  "main": "src/index.js",
  "author": "Wellington Cordeiro",
  "repository": "https://github.com/wldcordeiro/jest-runner-write-prettier",
  "license": "MIT",
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "jest",
    "precommit": "lint-staged",
    "prepush": "yarn test"
  },
  "peerDependencies": {
    "prettier": "^1.12.1"
  },
  "dependencies": {
    "cli-highlight": "^2.0.0",
    "create-jest-runner": "^0.3.1",
    "jest-diff": "^22.4.3",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-gitmoji-changelog": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-in-case": "^1.0.2",
    "jest-runner-eslint": "^0.5.0",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "rxjs-compat": "^6.1.0",
    "semantic-release": "^15.4.1",
    "snapshot-diff-serializer": "^1.0.0"
  },
  "jest": {
    "projects": ["jest-lint.config.js", "jest-prettier.config.js"],
    "snapshotSerializers": ["snapshot-diff-serializer"]
  },
  "lint-staged": {
    "*": "yarn jest"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "fix": true
    }
  }
}
