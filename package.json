{
  "name": "jest-runner-write-prettier",
  "version": "1.0.1",
  "description": "A Jest Runner for Prettier that writes the changes.",
  "main": "src/index.js",
  "author": "Wellington Cordeiro",
  "repository": "https://github.com/wldcordeiro/jest-runner-write-prettier",
  "license": "MIT",
  "scripts": {
    "semantic-release": "semantic-release",
    "test": "jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test"
  },
  "peerDependencies": {
    "prettier": "^1.12.1"
  },
  "dependencies": {
    "create-jest-runner": "^0.3.1",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-in-case": "^1.0.2",
    "jest-runner-eslint": "^0.5.0",
    "prettier": "^1.12.1",
    "rxjs-compat": "^6.1.0",
    "semantic-release": "^15.4.1"
  },
  "jest": {
    "projects": ["jest-*.config.js"]
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "fix": true
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
